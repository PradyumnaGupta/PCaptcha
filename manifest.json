{
    "manifest_version": 2,
    "name": "Custom Google Homepage",
    "description": "This extension changes the background image of the google homepage",
    "version": "1.0",
    "permissions": [
        "topSites", "history", "browsingData"
    ],
    "content_script"
    "background":{
        "scripts":[
            "topSites.js", "mouse.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup1.html"
    }
   }