{
    "manifest_version": 2,
    "name": "PCaptcha - A CAPTCHA For Blind",
    "description": "This extension adds a script to page that automatically tries to differentiate between bots and humans without using Captcha",
    "version": "2.0",
    "icons": { "16": "./icons/icon16.png",
                "48": "./icons/icon48.png",
                "128": "./icons/icon128.png" 
    },
    "permissions": [
        "topSites", "history", "browsingData"
    ],
    "content_scripts": [
        {
          "matches": ["https://example.com/*"],
          "js": ["DOMinjection.js", "metaSession.js", "mouse.js", "confidence_score.js"]
        }
      ],
    "background":{
        "scripts":[
            "topSites.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
   }